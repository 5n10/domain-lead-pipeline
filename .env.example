DATABASE_URL=postgresql+psycopg2://postgres:postgres@localhost:5432/domain_leads
BATCH_SIZE=100
DNS_TIMEOUT=5
DNS_CHECK_WWW=true
HTTP_TIMEOUT=10
HTTP_USER_AGENT=domain-lead-pipeline/0.1
TCP_PROBE_ENABLED=true
TCP_PROBE_TIMEOUT=3
TCP_PROBE_PORTS=80,443
AUTO_RUNNER_ENABLED=false
AUTO_RUNNER_INTERVAL_SECONDS=900
MUTATION_API_KEY=
MUTATION_LOCALHOST_BYPASS=true
AUTO_DAILY_TARGET_ENABLED=true
DAILY_TARGET_COUNT=100
DAILY_TARGET_MIN_SCORE=40
DAILY_TARGET_PLATFORM_PREFIX=daily
DAILY_TARGET_REQUIRE_CONTACT=true
DAILY_TARGET_REQUIRE_DOMAIN_QUALIFICATION=false
DAILY_TARGET_REQUIRE_UNHOSTED_DOMAIN=false
DAILY_TARGET_ALLOW_RECYCLE=true
RDAP_BASE_URL=https://rdap.org/domain/
OVERPASS_ENDPOINT=https://overpass-api.de/api/interpreter
OVERPASS_TIMEOUT=180
OVERPASS_FILTER_CHUNK=3
OVERPASS_ENDPOINTS=https://overpass-api.de/api/interpreter,https://overpass.kumi.systems/api/interpreter,https://overpass.nchc.org.tw/api/interpreter
OVERPASS_ELEMENT_TYPES=node
OVERPASS_RETRIES=3
OVERPASS_RETRY_DELAY=5
OVERPASS_SLEEP=1
OVERPASS_BBOX_SPLIT=1

# Optional paid/free-tier API keys
WHOISXML_API_KEY=
DOMAINTOOLS_API_KEY=
HUNTER_API_KEY=
APOLLO_API_KEY=
INSTANTLY_API_KEY=
LEMLIST_API_KEY=

# Google Places API (New) — 10,000 free calls/month on Essentials tier
# Get your key at: https://console.cloud.google.com/apis/credentials
# Enable "Places API (New)" in your project
GOOGLE_PLACES_API_KEY=

# Foursquare Places API — 10,000 free calls/month
FOURSQUARE_API_KEY=

# LLM verification (any one of these enables LLM verify)
OPENROUTER_API_KEY=
GEMINI_API_KEY=
GROQ_API_KEY=

# Push notifications via ntfy.sh (free, no registration)
NTFY_TOPIC=
NTFY_SERVER=https://ntfy.sh

# Google Sheets export (requires service account)
GOOGLE_SHEETS_CREDENTIALS_FILE=
GOOGLE_SHEETS_SPREADSHEET_ID=

# CSV export
EXPORT_DIR=./exports
